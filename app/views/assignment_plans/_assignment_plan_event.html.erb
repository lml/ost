{
  title  : '<%= assignment_plan.name %>',
  start  : '<%= TimeUtils.time_and_zonestr_to_timestr_in_zone(assignment_plan.starts_at, assignment_plan.learning_plan.klass.time_zone, "%Y-%m-%d %H:%M") %>',
  end  : '<%= TimeUtils.time_and_zonestr_to_timestr_in_zone(assignment_plan.ends_at, assignment_plan.learning_plan.klass.time_zone, "%Y-%m-%d %H:%M") %>',
  allDay : false,
  editable: true,
  ost_id : <%= assignment_plan.id %>,
  id: 'assignment_plan_<%= assignment_plan.id %>',
  color: <%= assignment_plan.is_test ? "get_os_color('orange')".html_safe : "get_os_color('blue')".html_safe %>
}
